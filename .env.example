# AI路由器环境变量配置
APP_DESCRIPTION=智能AI路由器 - 为大型语言模型提供统一API接口!!!

# 应用配置
DEBUG=false
HOST=0.0.0.0
PORT=8000

# PostgreSQL数据库配置
DATABASE_URL=postgresql://user:password@localhost:5432/database

# 负载均衡配置
LOAD_BALANCING_STRATEGY=performance_based
LOAD_BALANCING_HEALTH_CHECK_INTERVAL=60
LOAD_BALANCING_MAX_RETRIES=5

# Redis配置
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=<PASSWORD>
REDIS_DB=0

# 安全配置
RATE_LIMIT=100
API_KEY_REQUIRED=true 

